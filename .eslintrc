{
  "rules": {
    "indent": [
            2,
            2
        ],
    "quotes": [
            2,
            "single"
        ],
    "linebreak-style": [
            2,
            "unix"
        ],
    "semi": [
            2,
            "always"
        ],
    "max-len": [2, 80, 2, {
      "ignoreComments": true,
      "ignoreUrls": true,
        }]

  },
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "modules": true
  },
  "fix": true
}
